= fields_for 'educational_qualifications[]', educational_qualification, include_id: false do |f|
  %table.table{ id: "#{table_id}", style: "#{table_visibility}"}
    %tr
      %td.thirty-three
        .form-group
          = f.label :level_of_education
          = f.text_field :level_of_education, class: 'form-control'
      %td.one
      %td.thirty-three
        .form-group
          = f.label :institution
          = f.text_field :institution, class: 'form-control'
      %td.one
      %td.thirty-three
        .form-group
          = f.label :roll
          = f.text_field :roll, class: 'form-control'
    %tr
      %td.thirty-three
        .form-group
          = f.label :result
          = f.text_field :result, class: 'form-control'
      %td.one
      %td.thirty-three
        .form-group
          = f.label :year
          = f.text_field :year, class: 'form-control'
      %td.one
      %td.thirty-three
        .form-group
          = f.label :duration
          = f.text_field :duration, class: 'form-control'
    %tr
      %td.thirty-three
        .form-group
          = f.label :country_name, 'Country'
          = f.text_field :country_name, class: 'form-control'
      -#%tr
      -#  %td
      -#    = button_tag 'Remove', class: 'btn btn-danger pull-left', id: 'remove-form'